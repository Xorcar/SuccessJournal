<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="294.0" prefWidth="476.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.GroupFullWinController">
   <children>
      <ScrollPane prefHeight="200.0" prefWidth="200.0">
         <content>
            <TableView fx:id="tblJournal" prefHeight="200.0" />
         </content>
      </ScrollPane>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Label text="З " />
            <DatePicker fx:id="dteFrom" editable="false" prefHeight="31.0" prefWidth="128.0" />
            <Label text="по " />
            <DatePicker fx:id="dteTo" editable="false" prefHeight="31.0" prefWidth="125.0" />
            <CheckBox fx:id="cheAll" mnemonicParsing="false" text="Усі дні." />
            <Button fx:id="btnRefresh" mnemonicParsing="false" text="Оновити" />
         </children>
      </HBox>
      <Button fx:id="btnEdit" mnemonicParsing="false" text="Редагувати" />
   </children>
</VBox>
